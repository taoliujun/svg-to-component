    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-resizable-panels": "^1.0.9",
    "react-router-dom": "^6.15.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0",
    "webpack-plugin-noop-ts": "^2.1.6",
    "@types/lodash": "^4.14.195",
