    "ahooks": "^3.7.8",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "chalk": "^4.1.2",
    "classnames": "^2.3.2",
    "core-js": "^3.32.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.1",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lodash": "^4.17.21",
